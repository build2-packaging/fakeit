#pragma once

// The numeric version format is AAAAABBBBBCCCCCDDDE where:
//
// AAAAA - major version number
// BBBBB - minor version number
// CCCCC - bugfix version number
// DDD   - alpha / beta (DDD + 500) version number
// E     - final (0) / snapshot (1)
//
// When DDDE is not 0, 1 is subtracted from AAAAABBBBBCCCCC. For example:
//
// Version      AAAAABBBBBCCCCCDDDE
//
// 0.1.0        0000000001000000000
// 0.1.2        0000000001000020000
// 1.2.3        0000100002000030000
// 2.2.0-a.1    0000200001999990010
// 3.0.0-b.2    0000299999999995020
// 2.2.0-a.1.z  0000200001999990011
//
#define FAKEIT_VERSION       $fakeit.version.project_number$ULL
#define FAKEIT_VERSION_STR   "$fakeit.version.project$"
#define FAKEIT_VERSION_ID    "$fakeit.version.project_id$"
#define FAKEIT_VERSION_FULL  "$fakeit.version$"

#define FAKEIT_VERSION_MAJOR $fakeit.version.major$
#define FAKEIT_VERSION_MINOR $fakeit.version.minor$
#define FAKEIT_VERSION_PATCH $fakeit.version.patch$

#define FAKEIT_PRE_RELEASE   $fakeit.version.pre_release$

#define FAKEIT_SNAPSHOT_SN   $fakeit.version.snapshot_sn$ULL
#define FAKEIT_SNAPSHOT_ID   "$fakeit.version.snapshot_id$"
